# Copy this file to .env.local before running Vite.
# API base (key used by this frontend)
VITE_API_BASE=http://127.0.0.1:3000/api
VITE_ADMIN_API_BASE=http://127.0.0.1:3000/api
# optional alias for some setups/tools
VITE_API_BASE_URL=http://127.0.0.1:3000

# Tenant defaults
VITE_TENANT=bn9
VITE_DEFAULT_TENANT=bn9
VITE_DEFAULT_BOT_ID=dev-bot

# Build metadata
VITE_APP_VERSION=dev

# Optional admin helper (used occasionally for maintenance only)
VITE_ADMIN_CODE=

# Optional OpenAI keys for local AI-assisted workflows
VITE_OPENAI_API_KEY=
VITE_OPENAI_MODEL=gpt-4o-mini
